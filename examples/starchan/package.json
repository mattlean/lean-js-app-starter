        "check-types": "tsc",
        "dev": "concurrently --names \"ğŸ“¦ webpack (back),ğŸŒ express,ğŸ¨ dev-server (front)\" \"nodemon  --config backend.nodemon.json --exec 'npm run backend:build -- --watch'\" \"nodemon --watch build/backend build/backend/server.js\" \"npm run frontend:dev\"",
