        "format:write": "prettier --write .",
        "front:build": "npm run frontend:build",
        "front:dev": "npm run frontend:dev",
        "frontend:build": "webpack --config webpack/frontend.config.js --mode development",
        "frontend:dev": "nodemon --config frontend.nodemon.json --exec \"webpack serve --config webpack/frontend.config.js --mode development\"",
        "lint": "eslint .",
        "lint:fix": "npm run lint -- --fix",
        "prisma": "prisma",
        "prisma:studio": "npm run prisma studio -- --browser none",
        "start": "node build/backend/server.js",
        "start:debug": "nodemon --inspect=0.0.0.0:9229 --watch build/backend build/backend/server.js",
        "test": "jest",
        "test:coverage": "npm run test -- --coverage",
        "test:watch": "npm run test -- --watch || npm run test -- --watchAll"
    },
