        "back:build": "npm run backend:build",
        "back:build:production": "npm run backend:build:production",
        "back:build:watch": "npm run backend:build:watch",
        "backend:build": "webpack --config webpack/backend.config.js --mode development",
        "backend:build:production": "webpack --config webpack/backend.config.js --mode production",
        "backend:build:watch": "nodemon  --config backend.nodemon.json --exec \"npm run backend:build -- --watch\"",
        "backend:dev": "concurrently --names \"ğŸ“¦ webpack (back),ğŸŒ express\" \"npm run backend:build:watch\" \"npm run start:debug\"",
        "build": "npm run frontend:build && npm run backend:build",
        "build:production": "npm run frontend:build && npm run backend:build:production",
        "check-types": "tsc",
        "dev": "concurrently --names \"ğŸ“¦ webpack (back),ğŸŒ express,ğŸ¨ dev-server (front)\" \"npm run backend:build:watch\" \"npm run start:debug\" \"npm run frontend:dev\"",
