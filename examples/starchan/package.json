        "back:build": "npm run backend:build",
        "back:start": "npm run backend:start",
        "backend:build": "webpack --config webpack/backend.config.js --mode development",
        "backend:dev": "concurrently --names \"ğŸ“¦ webpack (back),ğŸŒ express\" \"nodemon  --config backend.nodemon.json --exec 'npm run backend:build -- --watch'\" \"nodemon --watch build/backend build/backend/server.js\"",
