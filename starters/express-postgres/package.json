{
    "name": "ljas-express-postgres",
    "version": "0.2.0",
    "description": "",
    "main": "build/server.js",
    "author": "",
    "scripts": {
        "build": "webpack --mode development",
        "build:debug": "node --inspect-brk=0.0.0.0:9231 node_modules/.bin/webpack --mode development",
        "build:production": "webpack --mode production",
        "build:production:debug": "node --inspect-brk=0.0.0.0:9231 node_modules/.bin/webpack --mode production",
        "build:stats": "npm run build:production -- --json > stats.json",
        "build:watch": "nodemon --exec \"npm run build -- --watch\"",
        "dev": "concurrently --names \"ğŸ“¦ webpack,ğŸŒ express\" \"npm run build:watch\" \"nodemon --watch build npm run start:debug\"",
        "format": "npm run format:check",
        "format:check": "prettier --check .",
        "format:write": "prettier --write .",
        "lint": "eslint .",
        "lint:fix": "npm run lint -- --fix",
        "lint-staged": "lint-staged",
        "prisma": "prisma",
        "prisma:studio": "npm run prisma studio -- --browser none",
        "start": "node build/server.js",
        "start:debug": "nodemon --inspect=0.0.0.0:9229 --watch build build/server.js",
        "test": "jest",
        "test:coverage": "npm run test -- --coverage",
        "test:coverage:open": "open coverage/lcov-report/index.html",
        "test:debug": "node --inspect-brk=0.0.0.0:9230 node_modules/.bin/jest --runInBand",
        "test:watch": "npm run test -- --watch || npm run test -- --watchAll"
    },
    "dependencies": {
        "@babel/cli": "^7.21.5",
        "@babel/core": "^7.22.1",
        "@babel/preset-env": "^7.22.4",
        "@prisma/client": "^4.15.0",
        "babel-loader": "^9.1.2",
        "compression": "^1.7.4",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "helmet": "^7.1.0",
        "ljas-webpack": "^0.1.1-experimental.9",
        "prisma": "^4.15.0",
        "webpack": "^5.85.0",
        "webpack-cli": "^5.1.1",
        "webpack-merge": "^5.9.0",
        "webpack-node-externals": "^3.0.0"
    },
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^4.3.0",
        "concurrently": "^8.2.0",
        "eslint": "^8.41.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-jest": "^27.2.1",
        "jest": "^29.5.0",
        "jest-mock-extended": "^3.0.4",
        "nodemon": "^2.0.22",
        "prettier": "3.2.5",
        "supertest": "^6.3.3"
    },
    "private": true
