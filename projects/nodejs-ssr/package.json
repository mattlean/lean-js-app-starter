{
  "name": "lean-js-app-starter",
  "version": "0.1.1",
  "description": "A starter project to kickoff development your next JavaScript application.",
  "main": "src/back/main.js",
  "scripts": {
    "setup": "npm i && npm run flow-typed install",
    "back:api:start:dev:watch": "nodemon --watch nodemon.json --watch config/back --watch config/common --watch src/back --exec \"npm run back:build:dev && npm run back:start:dev\"",
    "back:api:start:dev:watch:debug": "nodemon --watch nodemon.json --watch config/back --watch config/common --watch src/back --exec \"npm run back:build:dev && npm run back:start:dev:debug\"",
    "back:build": "webpack --env production --config config/back/index.js",
    "back:build:debug": "node --inspect-brk node_modules/.bin/webpack --env production --config config/back/index.js",
    "back:build:dev": "webpack --env development --config config/back/index.js",
    "back:build:dev:debug": "node --inspect-brk node_modules/.bin/webpack --env development --config config/back/index.js",
    "back:build:dev:stats": "npm run back:build:dev -- --json > stats.back.development.json",
    "back:build:stats": "npm run back:build -- --json > stats.back.production.json",
    "back:lint": "npm run back:lint:js",
    "back:lint:js": "eslint --cache \"{config/back,src/back}/**/*.{js,jsx}\"",
    "back:lint:styles": "stylelint src/back/**/*.scss src/back/**/*.css",
    "back:start": "node build/back/production/app.js",
    "back:start:debug": "node --inspect-brk build/back/production/app.js",
    "back:start:dev": "node build/back/development/app.js",
    "back:start:dev:debug": "node --inspect-brk build/back/development/app.js",
    "back:ssr:start:dev:watch": "nodemon --watch nodemon.json --watch config --watch src --exec \"npm run front:build:dev && npm run back:build:dev && npm run back:start:dev\"",
    "back:ssr:start:dev:watch:debug": "nodemon --watch nodemon.json --watch config --watch src --exec \"npm run front:build:dev && npm run back:build:dev && npm run back:start:dev:debug\"",
    "back:test": "jest src/back",
    "back:test:coverage": "jest src/back --coverage",
    "back:test:update": "jest src/back -u",
    "back:test:watch": "jest src/back --watch",
    "build": "npm run front:build && npm run back:build",
    "build:debug": "npm run front:build:debug && npm run back:build:debug",
    "build:dev": "npm run front:build:dev && npm run back:build:dev",
    "build:dev:debug": "npm run front:build:dev:debug && npm run back:build:dev:debug",
    "build:dev:stats": "npm run front:build:dev:stats && npm run back:build:dev:stats",
    "build:stats": "npm run front:build:stats && npm run back:build:stats",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "front:build": "webpack --env production --config config/front/index.js",
    "front:build:debug": "node --inspect-brk node_modules/.bin/webpack --env production --config config/front/index.js",
    "front:build:dev": "webpack --env development --config config/front/index.js",
    "front:build:dev:debug": "node --inspect-brk node_modules/.bin/webpack --env development --config config/front/index.js",
    "front:build:dev:stats": "npm run front:build:dev -- --json > stats.front.development.json",
    "front:build:stats": "npm run front:build -- --json > stats.front.production.json",
    "front:lint": "npm run front:lint:js",
    "front:lint:js": "eslint --cache \"{config/front,src/front}/**/*.{js,jsx}\"",
    "front:lint:styles": "stylelint src/front/**/*.scss src/front/**/*.css",
    "front:start": "http-server ./build/front/production -p 9090",
    "front:start:dev": "http-server ./build/front/development -p 8080",
    "front:start:dev:watch": "nodemon --watch nodemon.json --watch config/common --watch config/front --exec \"webpack-dev-server --env development --config config/front/index.js\"",
    "front:test": "jest src/front",
    "front:test:coverage": "jest src/front --coverage",
    "front:test:update": "jest src/front -u",
    "front:test:watch": "jest src/front --watch",
    "lint": "npm run lint:js",
    "lint:js": "eslint --cache \"{config,src}/**/*.{js,jsx}\"",
    "lint:styles": "stylelint src/**/*.scss src/**/*.css",
    "start": "npm run back:start",
    "start:debug": "npm run back:start:debug",
    "start:dev": "npm run back:start:dev",
    "start:dev:debug": "npm run back:start:dev:debug",
    "start:dev:watch": "npm run back:ssr:start:dev:watch",
    "start:dev:watch:debug": "npm run back:ssr:start:dev:watch:debug",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:update": "jest -u",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IsaacLean/lean-js-app-starter.git"
  },
  "keywords": [
    "lean-js-app-starter",
    "lean-start",
    "lean",
    "javascript",
    "starter",
    "project",
    "boilerplate",
    "js",
    "application",
    "app"
  ],
  "author": "Matthew Lean",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IsaacLean/lean-js-app-starter/issues"
  },
  "homepage": "https://github.com/IsaacLean/lean-js-app-starter#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cross-fetch": "^2.2.3",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "moment": "^2.22.2",
    "mongoose": "^5.3.14",
    "normalizr": "^3.3.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-hot-loader": "^4.3.12",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "source-map-support": "^0.5.9",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "asset-list-webpack-plugin": "^0.4.0",
    "autoprefixer": "^9.3.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.1",
    "cssnano": "^4.1.7",
    "deep-freeze": "0.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.9.0",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-jest": "^21.27.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "flow-bin": "^0.83.0",
    "flow-typed": "^2.5.1",
    "flow-webpack-plugin": "^1.2.0",
    "glob": "^7.1.3",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "jest": "^23.6.0",
    "jsdom": "^12.2.0",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.10.0",
    "nodemon": "^1.18.7",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "^7.0.6",
    "postcss-loader": "^3.0.0",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "react-test-renderer": "^16.6.3",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.9.0",
    "stylelint-config-recommended": "^2.1.0",
    "supertest": "^3.3.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.11",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  }
}
